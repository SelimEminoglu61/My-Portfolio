{
    "version": 2,
    "builds": [
      {
        "src": "/backend/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/backend/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "https://selim-eminoglu-portfolio.vercel.app"
        }
      }
    ]
  }