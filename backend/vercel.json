{  
    "version": 2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./index.js",
        "methods":["POST","GET","OPTIONS"],
        "headers":{
          "Access-Control-Allow-Origin": "https://selim-eminoglu-portfolio.vercel.app",
          "Access-Control-Allow-Headers":"Content-Type"
        }
      }
    ]
  }